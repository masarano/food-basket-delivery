{
  "name": "hug-moms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "gc": "ng generate component --spec false",
    "ng-serve": "ng serve",
    "ng-serve-noam-ip-10.0.0.6": "ng serve --host 10.0.0.6",
    "start": "npm run server:execute",
    "build": "npm run server:build && npm run ng-build",
    "ng-build": "ng build --prod",
    "server:build": "tsc -p tsconfig.server.json",
    "server:build-watch": "tsc -p tsconfig.server.json --watch",
    "publish-to-heroku-test": "git push heroku master -f",
    "publish-to-nrun": "git push nrun master -f",
    "publish-to-salmichal": "git push salmichal master -f",
    "publish-to-hmey": "git push hmey master -f",
    "publish-to-merkaz-mazon": "git push mmaz master -f",
    "publish-to-m-h": "git push m-h master -f",
    "copy-db": "heroku pg:copy hugmoms::DATABASE_URL hugmoms2::DATABASE_URL -a hugmoms2",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server:execute": "node dist-server/src/app/server/server.js",
    "server:debug": "node --inspect --debug-brk dist-server/src/app/server/server.js",
    "RunSomething": "node --inspect --debug-brk dist-server/src/app/server/doSomething.js",
    "server-watch": "ts-node-dev --respawn --project tsconfig.server.json  src/app/server/server.ts",
    "server-build-and-run": "npm run server:build && npm run server:execute"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.9",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "^7.2.9",
