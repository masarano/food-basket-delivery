    "@angular/platform-browser-dynamic": "^7.2.9",
    "@angular/router": "^7.2.9",
    "@auth0/angular-jwt": "^2.0.0",
    "body-parser": "^1.18.3",
    "bootstrap3": "^3.3.5",
    "chart.js": "^2.7.3",
    "compression": "^1.7.3",
    "copy-to-clipboard": "^3.0.8",
    "core-js": "^2.5.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-force-https": "^1.0.0",
    "express-sse": "^0.5.0",
    "googlemaps": "^1.12.0",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^8.3.0",
